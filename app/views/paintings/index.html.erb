<h1>Painting Gallery</h1>

<div id="paintings">
  <%= render @paintings %>
</div>
<div class="clear"></div>

<%= form_for Painting.new do |f| %>
  <%= f.label :image, "Upload images:" %>
  <%= f.file_field :image, multiple: true, name: "painting[image]" %>
<% end %>

<div id="center" >
  <p1>Templates</p1>
  <div>
    <button class="templates_buttons" id="template1">Template1</button>
    <button class="templates_buttons" id="template2">Template2</button>
    <button class="templates_buttons" id="template3">Template3</button>
    <img src="images/pl.gif" onclick="zoom_in()" style="vertical-align: top; cursor: pointer" border="0" height="29" width="25">
    <img src="images/mn.gif" onclick="zoom_out()" style="vertical-align: top; cursor: pointer" border="0" height="29" width="25">
    <img src="images/mn.gif" onclick="delete_img()" style="vertical-align: top; cursor: pointer" border="0" height="29" width="25">

  </div>
  <canvas id="111" width="400" height="400" style="background-color: #999999">
  </canvas>
  <%=render 'templates/template1' %>
  <%=render 'templates/template2' %>
  <%=render 'templates/template3' %>
</div>

<script id="template-upload" type="text/x-tmpl">
<div class="upload">
  {%=o.name%}
  <div class="progress"><div class="bar" style="width: 0%"></div></div>
</div>
</script>